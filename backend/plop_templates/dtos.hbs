import { IsEmail, IsString, IsNotEmpty, MinLength, MaxLength } from 'class-validator';

export class Create{{camel name}}Dto {
  @IsEmail()
  public email: string;

  @IsString()
  @IsNotEmpty()
  @MinLength(9)
  @MaxLength(32)
  public password: string;
}

export class Update{{camel name}}Dto {
  @IsString()
  @IsNotEmpty()
  @MinLength(9)
  @MaxLength(32)
  public password: string;
}
